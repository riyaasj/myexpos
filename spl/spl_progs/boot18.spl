//INIT PROCESS WITH PID=1
//PAGE TABLE
PTBR= PAGE_TABLE_BASE+20;
PTLR=10;

//lib
[PTBR+0] = 63;
[PTBR+1] = "0100";
[PTBR+2] = 64;
[PTBR+3] = "0100"; 
//heap
[PTBR+4] = 78;
[PTBR+5] = "0110"; 
[PTBR+6] = 79;
[PTBR+7] = "0110"; 
//code
[PTBR+8] = 65;
