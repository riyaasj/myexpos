[PTBR+10] = 66;
[PTBR+11] = "0100";
[PTBR+12] = -1;
[PTBR+13] = "0000"; 
[PTBR+14] = -1;
[PTBR+15] = "0000"; 
//stack
[PTBR+16] = 76;
[PTBR+17] = "0110";
[PTBR+18] = 77;
[PTBR+19] = "0110";

//Process Table

[PROCESS_TABLE + 27] = 80;
[PROCESS_TABLE + 17] = 1;
[PROCESS_TABLE + 29] = 8*512;
[PROCESS_TABLE + 20] = CREATED;
[PROCESS_TABLE + 28] = 0;
[PROCESS_TABLE + 30] = PTBR;
[PROCESS_TABLE + 31] = 10;

//PER PROCEESS RESOURCE TABLE
R1=80*512+496;    //496=511-16+1
while(R1<=80*512+511) do
    [R1]=-1;
    R1=R1+1;
endwhile;

//Mem Free List
//initialise to 0
R1=0;				//reserved for OS
while(R1<=75) do 	
	[57*512+R1]=1;
	R1=R1+1;
endwhile;
R1=76;				//free  pages
while(R1<=127) do 	
	[57*512+R1]=0;
	R1=R1+1;
endwhile;
R1=76;
while(R1<=82) do
	[57*512+R1]=1;
	R1=R1+1;
endwhile;

//Initialize the fields WAIT_MEM_COUNT to 0 and MEM_FREE_COUNT to number of free pages in the system status table.
[SYSTEM_STATUS_TABLE+2]=45;
[SYSTEM_STATUS_TABLE+3]=0;

[76*512] = [65*512 + 1]; //IP in stack

[TERMINAL_STATUS_TABLE] = 0;
[DISK_STATUS_TABLE] = 0;

//Process table entries of other processes
alias id R1;
id=2;
while(id<=15)	do
	[PROCESS_TABLE + 16*id + 4] = TERMINATED;
	id = id+1;
endwhile;
return;
